{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "powershell -Command \"$proc = Get-NetTCPConnection -LocalPort 3000 -ErrorAction SilentlyContinue | Select-Object -ExpandProperty OwningProcess | Get-Unique; if ($proc) { Stop-Process -Id $proc -Force; Start-Sleep -Seconds 2 }; npm run dev:next\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test-wallet": "tsx test-wallet-balance.ts",
    "test-bingx": "tsx test-bingx-connector.ts",
    "dev:next": "next dev",
    "restart": "for /f \"tokens=5\" %a in ('netstat -aon ^| findstr :3000 ^| findstr LISTENING') do taskkill /F /PID %a & timeout /t 2 /nobreak & next dev"
  },
  "dependencies": {
    "@prisma/client": "^6.10.0",
    "@types/node-cron": "^3.0.11",
    "bcryptjs": "^2.4.3",
    "bybit-api": "^4.3.1",
    "ccxt": "^4.5.11",
    "dotenv": "^17.2.3",
    "google-auth-library": "^10.4.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "mexc-futures-sdk": "^1.5.1",
    "next": "15.5.2",
    "next-auth": "^4.24.13",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "protobufjs": "^7.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "undici": "^7.16.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/swc-wasm-nodejs": "^15.5.2",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "19.2.2",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "jest": "^30.2.0",
    "prisma": "^6.10.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.5",
    "typescript": "5.9.3"
  }
}
